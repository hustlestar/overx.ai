{
  "buildCommand": "cd ../.. && npm install && npm run build:converter",
  "outputDirectory": ".next",
  "installCommand": "cd ../.. && npm install",
  "framework": "nextjs",
  "redirects": [
    {
      "source": "/:path*",
      "has": [
        {
          "type": "host",
          "value": "converter.overx.ai"
        }
      ],
      "destination": "https://rates.overx.ai/:path*",
      "permanent": true
    }
  ]
}